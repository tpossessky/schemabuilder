<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableView?>
<AnchorPane prefHeight="611.0" prefWidth="724.0" stylesheets="@stylesheet.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AuthorPaneController">

    <Pane prefHeight="75.0" prefWidth="724.0" style="-fx-background-color: #DCDCDC;">
        <Label layoutX="11.0" layoutY="26.0" text="Authors" textFill="#7c8184">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Label fx:id="deleteAuthor" layoutX="646.0" layoutY="25.0" onMouseClicked="#deleteAuthor" text="Delete" textFill="#7c8184">
            <padding>
                <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
            </padding>
        </Label>
        <Label fx:id="createAuthor" layoutX="548.0" layoutY="25.0" onMouseClicked="#createAuthor" text="+ Create New" textFill="#7c8184">
            <padding>
                <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
            </padding>
        </Label>

    </Pane>
    <TableView fx:id="authorTable" layoutY="75.0" prefHeight="526.0" prefWidth="724.0" />
</AnchorPane>
